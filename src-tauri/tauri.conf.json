{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Goel",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "notification": {
        "all": true
      },
      "all": false,
      "process": {
          "exit": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
          "startDragging": true,
          "hide": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "de.jacobmellin.goel",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "systemTray" : {
        "iconPath": "icons/icon.png",
        "menuOnLeftClick": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "goel",
        "width": 840,
        "minWidth": 430,
        "height": 600,
        "minHeight": 600,
        "transparent": true,
        "decorations": false
      }
    ]
  }
}
